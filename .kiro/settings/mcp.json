{
  "mcpServers": {
    "Context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "*",
        "create_entities",
        "create_relations",
        "add_observations",
        "delete_entities",
        "delete_observations",
        "delete_relations",
        "read_graph",
        "search_nodes",
        "open_nodes",
        "resolve-library-id",
        "get-library-docs"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "disabled": false,
      "autoApprove": [
        "*",
        "search_repositories",
        "get_file_contents",
        "list_commits",
        "create_repository",
        "create_or_update_file",
        "push_files",
        "create_issue",
        "create_pull_request",
        "list_issues",
        "get_issue",
        "get_pull_request",
        "fork_repository",
        "create_branch",
        "update_issue",
        "add_issue_comment",
        "search_code",
        "search_issues",
        "search_users",
        "list_pull_requests",
        "create_pull_request_review",
        "merge_pull_request",
        "get_pull_request_files",
        "get_pull_request_status",
        "update_pull_request_branch",
        "get_pull_request_comments",
        "get_pull_request_reviews"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "*",
        "sequentialthinking"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "*",
        "create_entities",
        "create_relations",
        "add_observations",
        "delete_entities",
        "delete_observations",
        "delete_relations",
        "read_graph",
        "search_nodes",
        "open_nodes"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users",
        "."
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "*",
        "read_file",
        "read_multiple_files",
        "write_file",
        "edit_file",
        "create_directory",
        "list_directory",
        "list_directory_with_sizes",
        "directory_tree",
        "move_file",
        "search_files",
        "get_file_info",
        "list_allowed_directories"
      ]
    },
    "browsermcp": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "disabled": false,
      "autoApprove": [
        "*",
        "browser_navigate",
        "browser_go_back",
        "browser_go_forward",
        "browser_snapshot",
        "browser_click",
        "browser_hover",
        "browser_type",
        "browser_select_option",
        "browser_press_key",
        "browser_wait",
        "browser_get_console_logs",
        "browser_screenshot",
        "puppeteer_navigate",
        "puppeteer_screenshot",
        "puppeteer_evaluate",
        "puppeteer_click",
        "puppeteer_fill"
      ]
    },
    "Homebrew": {
      "command": "brew",
      "args": [
        "mcp-server"
      ],
      "disabled": false,
      "autoApprove": [
        "*",
        "install"
      ]
    },
    "super-shell": {
      "command": "npx",
      "args": [
        "-y",
        "super-shell-mcp"
      ],
      "disabled": false,
      "autoApprove": [
        "*",
        "execute_command",
        "get_whitelist",
        "add_to_whitelist"
      ]
    },
    "hf-mcp-server": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://huggingface.co/mcp",
        "--header",
        "--transport",
        "sse-only",
        "Authorization: Bearer ${HF_TOKEN}"
      ],
      "disabled": false,
      "autoApprove": [
        "model_search",
        "model_details"
      ]
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "*",
        "fetch"
      ]
    },
    "Figma MCP": {
      "command": "npx",
      "args": [
        "-y",
        "figma-developer-mcp",
        "--figma-api-key=${FIGMA_API_KEY}",
        "--stdio"
      ],
      "env": {
        "FIGMA_API_KEY": "${FIGMA_API_KEY}"
      },
      "disabled": true,
      "autoApprove": [
        "*",
        "get_figma_data"
      ]
    }
  }
}